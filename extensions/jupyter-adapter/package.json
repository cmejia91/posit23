{
  "name": "jupyter-adapter",
  "displayName": "%displayName%",
  "description": "%description%",
  "publisher": "vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Jupyter Heartbeat Interval",
      "properties": {
        "positron.jupyter.heartbeat": {
          "scope": "window",
          "type": "number",
          "default": 10,
          "description": "The interval between kernel heartbeat requests (0 to disable heartbeat checks)"
        }
      }
    }
  },
  "scripts": {
    "compile": "gulp compile-extension:jupyter-adapter",
    "postinstall": "ts-node scripts/postinstall.ts",
    "watch": "gulp watch-extension:jupyter-adapter",
    "vscode:prepublish": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../../build/gulpfile.extensions.js compile-extension:search-result ./tsconfig.json"
  },
  "devDependencies": {
    "@types/tail": "^2.2.1",
    "@types/uuid": "^8.3.3",
    "@types/zeromq": "^5.2.1",
    "electron-rebuild": "^3.2.9",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "portfinder": "^1.0.28",
    "tail": "^2.2.6",
    "uuid": "^9.0.0",
    "vscode-nls": "^5.2.0",
    "zeromq": "6.0.0-beta.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rstudio/positron"
  }
}
