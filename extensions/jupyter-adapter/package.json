{
  "name": "myriac-console",
  "displayName": "Myriac Console",
  "description": "Jupyter-Powered Console",
  "publisher": "RStudio",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:myriac-console.createConsole"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "myriac-console.createConsole",
        "title": "New Myriac Console"
      },
      {
        "command": "myriac-console.restartKernel",
        "title": "Restart Myriac Kernel"
      },
      {
        "command": "myriac-console.shutdownKernel",
        "title": "Shutdown Myriac Kernel"
      },
      {
        "command": "myriac-console.interruptKernel",
        "title": "Interrupt Myriac Kernel"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Myriac Console",
      "properties": {
        "myriac.heartbeat": {
          "scope": "window",
          "type": "number",
          "default": 10,
          "description": "The interval between kernel heartbeat requests (0 to disable heartbeat checks)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc -p ./",
    "compile:views": "webpack --mode development",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:views": "webpack --watch --mode development",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/uuid": "^8.3.3",
    "@types/vscode": "^1.61.0",
    "@types/zeromq": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "css-loader": "^6.5.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@internal/jupyter-wire": "file:src/wire",
    "@types/vscode-webview": "^1.57.0",
    "ansi-to-html": "^0.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "uuid": "^8.3.2",
    "zeromq": "^6.0.0-beta.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rstudio/myriac-console"
  }
}