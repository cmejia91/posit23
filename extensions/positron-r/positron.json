{
	"binaries": [
		{
			"from": "resources/branding/r-icon.svg",
			"to": "resources/branding"
		},
		{
			"from": "resources/scripts/startup.R",
			"to": "resources/scripts"
		},
		{
			"from": "resources/js/htmlwidget.js",
			"to": "resources/js"
		},
		{
			"from": "resources/ark/ark",
			"to": "./dist/bin"
		},
		{
			"from": "resources/ark/modules/public/*.R",
			"to": "./dist/bin/modules/public"
		},
		{
			"from": "resources/ark/modules/private/*.R",
			"to": "./dist/bin/modules/private"
		},
		{
			"from": "resources/testing/tree-sitter-r.wasm",
			"to": "resources/testing"
		},
		{
			"from": "resources/testing/vscodereporter/*",
			"to": "resources/testing"
		},
		{
			"from": "node_modules/web-tree-sitter/tree-sitter.wasm",
			"to":"./dist"
		}
	]
}
