{
  "name": "positron-editor-cells",
  "displayName": "Positron Editor Cells",
  "description": "Editor integrations for Jupyter percent and Knitr cell formats",
  "publisher": "vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "positron-editor-cells.goToPreviousCell",
        "title": "Go to Previous Cell",
        "category": "Python"
      },
      {
        "command": "positron-editor-cells.goToNextCell",
        "title": "Go to Next Cell",
        "category": "Python"
      },
      {
        "command": "positron-editor-cells.runCurrentAdvance",
        "title": "Run Cell and Advance",
        "category": "Python"
      },
      {
        "command": "positron-editor-cells.runCurrentCell",
        "title": "Run Cell",
        "category": "Python"
      },
      {
        "command": "positron-editor-cells.runAllCells",
        "title": "Run All Cells",
        "category": "Python"
      }
    ],
    "configuration": [
      {
        "title": "Positron Editor Cells",
        "properties": {}
      }
    ],
    "keybindings": [
      {
        "command": "positron-editor-cells.goToPreviousCell",
        "when": "editorTextFocus",
        "key": "ctrl+pageup",
        "mac": "cmd+pageup"
      },
      {
        "command": "positron-editor-cells.goToNextCell",
        "when": "editorTextFocus",
        "key": "ctrl+pagedown",
        "mac": "cmd+pagedown"
      },
      {
        "command": "positron-editor-cells.runCurrentAdvance",
        "key": "shift+enter",
        "when": "editorTextFocus && !findInputFocussed && !replaceInputFocussed"
      },
      {
        "command": "positron-editor-cells.runCurrentCell",
        "key": "ctrl+shift+enter",
        "mac": "cmd+shift+enter",
        "when": "editorTextFocus && !findInputFocussed && !replaceInputFocussed"
      },
      {
        "command": "positron-editor-cells.runAllCells",
        "key": "ctrl+alt+r",
        "mac": "cmd+alt+r",
        "when": "editorTextFocus && !findInputFocussed && !replaceInputFocussed"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/posit-dev/positron"
  }
}
