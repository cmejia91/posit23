{
  "name": "positron-notebook-controllers",
  "displayName": "Positron Notebook Controllers",
  "description": "Notebook Controllers for Positron Language Runtimes",
  "publisher": "vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Notebooks"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": [
      {
        "title": "Positron Notebook Controllers",
        "properties": {
          "python.languageServer": {
            "type": "string",
            "default": "Default",
            "description": "%python.languageServer.description%"
          }
        }
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/posit-dev/positron"
  }
}
